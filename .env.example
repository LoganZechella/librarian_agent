# Copy this file to `.env` and fill in the values before running the Librarian Agent.

# OpenAI API Key
OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# MongoDB Atlas connection string and database
MONGODB_ATLAS_URI=YOUR_MONGODB_ATLAS_URI
# (Optional) Database name (defaults to `librarian_kb` if not set)
MONGODB_DB=YOUR_MONGODB_DB

# AWS credentials for S3 (if using S3 storage)
AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY_ID
AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_ACCESS_KEY
# AWS region (e.g., us-west-2)
AWS_DEFAULT_REGION=YOUR_AWS_DEFAULT_REGION

# S3 bucket name for document storage
S3_BUCKET=YOUR_S3_BUCKET_NAME